package cn.mldn.util;

public class StringUtils {
	private StringUtils(){}
	public static String fistUpper(String str){
		if(str==null||"".equals(str)){
			return str;
		}
		if(str.length()==1)
			return str.toUpperCase();
		return str.substring(0,1).toUpperCase()+str.substring(1);
	}
}
